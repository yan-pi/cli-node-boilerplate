{
  "name": "teste",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "tsx src/server.ts",
    "build": "tsup src",
    "start:dev": "tsx watch src/server.ts",
    "husky:prepare": "npx husky init",
    "test": "vitest",
    "test:lint": "vitest run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn eslint --fix",
      "yarn prettier --write \"src/**/*.{ts,tsx}\"",
      "yarn test:lint --passWithNoTests"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "zod": "3.23.6",
    "prettier": "3.2.5",
    "eslint-config-prettier": "9.1.0",
    "@fastify/cors": "9.0.1",
    "@fastify/swagger": "8.14.0",
    "@fastify/swagger-ui": "3.0.0",
    "fastify-type-provider-zod": "1.1.9",
    "fastify": "4.26.2"
  },
  "devDependencies": {
    "@types/node": "20.12.8",
    "tsup": "8.0.2",
    "tsx": "4.9.3",
    "typescript": "5.4.5",
    "@eslint/js": "9.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint": "9.2.0",
    "@typescript-eslint/parser": "7.8.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "prettier": "3.2.5",
    "typescript-eslint": "7.8.0",
    "globals": "15.1.0",
    "vitest": "1.6.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2"
  }
}
